{
  "author": "@adamdotai",
  "repository": "https://github.com/adamdotai/date-io",
  "registry": "https://npm.pkg.github.com/",
  "name": "@adamdotai/date-io",
  "private": true,
  "version": "1.0.0",
  "description": "Abstraction over common javascript date management libraries",
  "main": "index.js",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "test": "cross-env TZ='UTC' NODE_ICU_DATA='./node_modules/full-icu' jest --watch",
    "test:coverage": "cross-env TZ='UTC' NODE_ICU_DATA='./node_modules/full-icu' jest --coverage",
    "test:windows": "test.cmd",
    "benchmark": "lerna run benchmark",
    "release": "yarn test:coverage && yarn build && lerna publish",
    "build": "lerna run build --parallel",
    "prettier": "prettier --write './packages/**/*.{js,jsx,ts,tsx,json,css,md,mdx}'",
    "build:readme": "node scripts/buildReadme.js"
  },
  "keywords": [
    "date",
    "time",
    "date-io",
    "picker",
    "date-fns",
    "moment",
    "luxon",
    "dayjs"
  ],
  "license": "MIT",
  "dependencies": {
    "@types/jest": "^26.0.0",
    "codecov": "^3.6.1",
    "full-icu": "^1.3.0",
    "husky": "^5.0.9",
    "jest": "^25.1.0",
    "lerna": "^3.19.0",
    "lint-staged": "^10.0.0",
    "prettier": "^2.0.1",
    "rollup": "^2.0.2",
    "rollup-plugin-typescript": "^1.0.1",
    "ts-jest": "^25.1.0",
    "typescript": "^4.3.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,json,css,md}": [
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "yarn build:readme"
    ]
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.2.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/react-hooks": "^7.0.1",
    "cross-env": "^7.0.3",
    "react": "^16.9.0",
    "react-test-renderer": "^16.9.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "set-tz": "^0.2.0",
    "tslib": "^2.3.0"
  }
}
